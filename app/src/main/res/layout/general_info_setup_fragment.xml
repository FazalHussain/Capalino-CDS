<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:src="@drawable/logo2"
        android:id="@+id/logo2"
        android:layout_marginLeft="16dp"
        />

    <ImageView
        android:id="@+id/logo"
        android:src="@drawable/logo"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="16dp"
        />
    <!--<ImageView
        android:id="@+id/logo"
        android:layout_centerHorizontal="true"
        android:src="@drawable/logo"
        android:layout_width="wrap_content"
        android:layout_height="30dp" />-->

    <RelativeLayout
        android:id="@+id/header"
        android:layout_below="@+id/logo"
        android:background="@color/text_color"
        android:layout_width="match_parent"
        android:layout_height="40dp">



        <com.MWBE.Connects.NY.CustomViews.CustomTextView
            android:textColor="@color/white"
            android:layout_centerInParent="true"
            android:text="@string/setup"
            android:textSize="18sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />



    </RelativeLayout>

    <RelativeLayout
        android:layout_above="@+id/footer"
        android:layout_below="@+id/header"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_alignParentBottom="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"  >

            <LinearLayout

                android:orientation="vertical"
                android:id="@+id/registerdialog"
                android:background="@drawable/round"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.MWBE.Connects.NY.CustomViews.CustomTextView_Bold
                    android:layout_marginTop="4dp"
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="40dp"
                    android:paddingRight="20dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:text="@string/step1"/>

                <com.MWBE.Connects.NY.CustomViews.CustomTextView_Book
                    android:layout_marginTop="4dp"
                    android:textSize="15sp"
                    android:layout_marginLeft="20dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp"
                    android:text="@string/step1_info"/>

                <com.MWBE.Connects.NY.CustomViews.CustomTextView_Book
                    android:layout_marginTop="10dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="4dp"
                    android:textSize="17sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/name"/>

                <com.MWBE.Connects.NY.CustomViews.CustomEditText_Book
                    android:padding="8dp"
                    android:id="@+id/fname"
                    android:textColor="@color/editext_color"
                    android:singleLine="true"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:background="@drawable/disabled_round_editext"
                    style="@style/App_EditTextStyle"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_width="match_parent"
                    android:layout_height="40dp" />

                <com.MWBE.Connects.NY.CustomViews.CustomTextView_Book
                    android:paddingTop="4dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:textSize="17sp"
                    android:paddingBottom="4dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/lastname"/>

                <com.MWBE.Connects.NY.CustomViews.CustomEditText_Book
                    android:padding="8dp"
                    android:id="@+id/lastname"
                    android:singleLine="true"
                    android:background="@drawable/disabled_round_editext"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:textColor="@color/editext_color"
                    style="@style/App_EditTextStyle"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_width="match_parent"
                    android:layout_height="40dp" />

                <com.MWBE.Connects.NY.CustomViews.CustomTextView_Book
                    android:paddingTop="4dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="4dp"
                    android:textSize="17sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/email"/>

                <com.MWBE.Connects.NY.CustomViews.CustomEditText_Book
                    android:padding="8dp"
                    android:id="@+id/email"
                    android:inputType="textEmailAddress"
                    android:singleLine="true"
                    style="@style/App_EditTextStyle"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:background="@drawable/disabled_round_editext"
                    android:textColor="@color/editext_color"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_width="match_parent"
                    android:layout_height="40dp" />

                <com.MWBE.Connects.NY.CustomViews.CustomTextView_Book
                    android:paddingTop="4dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="4dp"
                    android:textSize="17sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/phno"/>

                <com.MWBE.Connects.NY.CustomViews.CustomEditText_Book
                    android:padding="8dp"
                    android:singleLine="true"
                    android:inputType="number"
                    android:id="@+id/phno"
                    android:layout_marginLeft="20dp"
                    style="@style/App_EditTextStyle"
                    android:layout_marginRight="20dp"
                    android:textColor="@color/editext_color"
                    android:background="@drawable/round"
                    android:layout_width="match_parent"
                    android:layout_height="40dp" />

                <com.MWBE.Connects.NY.CustomViews.CustomTextView_Book
                    android:paddingTop="4dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="4dp"
                    android:textSize="17sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/buisnessname"/>

                <com.MWBE.Connects.NY.CustomViews.CustomEditText_Book
                    android:padding="8dp"
                    android:singleLine="true"
                    android:id="@+id/businessname"
                    android:inputType="textCapWords"
                    android:layout_marginLeft="20dp"
                    style="@style/App_EditTextStyle"
                    android:textColor="@color/editext_color"
                    android:layout_marginRight="20dp"
                    android:background="@drawable/round"
                    android:layout_width="match_parent"
                    android:layout_height="40dp" />

                <com.MWBE.Connects.NY.CustomViews.CustomTextView_Book
                    android:paddingTop="4dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="4dp"
                    android:textSize="17sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/buisnessaddress"/>

                <com.MWBE.Connects.NY.CustomViews.CustomEditText_Book
                    android:padding="8dp"
                    android:id="@+id/address"
                    android:singleLine="true"
                    style="@style/App_EditTextStyle"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:textColor="@color/editext_color"
                    android:inputType="textCapWords"
                    android:background="@drawable/round"
                    android:layout_width="match_parent"
                    android:layout_height="40dp" />

                <com.MWBE.Connects.NY.CustomViews.CustomTextView_Book
                    android:paddingTop="4dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="4dp"
                    android:textSize="17sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/buisnessweb"/>

                <com.MWBE.Connects.NY.CustomViews.CustomEditText_Book
                    android:padding="8dp"
                    android:singleLine="true"
                    android:id="@+id/businessweb"
                    android:inputType="textEmailAddress"
                    android:layout_marginLeft="20dp"
                    style="@style/App_EditTextStyle"
                    android:textColor="@color/editext_color"
                    android:layout_marginRight="20dp"
                    android:background="@drawable/round"
                    android:layout_width="match_parent"
                    android:layout_height="40dp" />

                <com.MWBE.Connects.NY.CustomViews.CustomTextView_Book
                    android:paddingTop="4dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="4dp"
                    android:textSize="17sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/city"/>

                <com.MWBE.Connects.NY.CustomViews.CustomEditText_Book
                    android:padding="8dp"
                    android:singleLine="true"
                    android:id="@+id/city"
                    android:inputType="textCapWords"
                    android:layout_marginLeft="20dp"
                    style="@style/App_EditTextStyle"
                    android:textColor="@color/editext_color"
                    android:layout_marginRight="20dp"
                    android:background="@drawable/round"
                    android:layout_width="match_parent"
                    android:nextFocusDown="@+id/zipcode"
                    android:layout_height="40dp" />

                <com.MWBE.Connects.NY.CustomViews.CustomTextView_Book
                    android:paddingTop="4dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="4dp"
                    android:textSize="17sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/zip"/>

                <com.MWBE.Connects.NY.CustomViews.CustomEditText_Book
                    android:padding="8dp"
                    android:id="@+id/zipcode"
                    android:inputType="number"
                    android:layout_marginLeft="20dp"
                    style="@style/App_EditTextStyle"
                    android:textColor="@color/editext_color"
                    android:layout_marginRight="20dp"
                    android:background="@drawable/round"
                    android:layout_width="match_parent"
                    android:layout_height="40dp" />

                <com.MWBE.Connects.NY.CustomViews.CustomTextView_Book
                    android:paddingTop="4dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="4dp"
                    android:textSize="17sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/state"/>

                <Spinner
                    android:paddingBottom="4dp"
                    android:singleLine="true"
                    android:id="@+id/state"
                    android:inputType="text"
                    android:layout_marginLeft="20dp"
                    style="@style/App_EditTextStyle"
                    android:textColor="@color/editext_color"
                    android:layout_marginRight="20dp"
                    android:background="@drawable/round"
                    android:layout_width="match_parent"
                    android:layout_height="40dp" />

                <LinearLayout
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <com.MWBE.Connects.NY.CustomViews.CustomButton
                        android:textColor="@color/white"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:id="@+id/next"
                        android:text="Next"
                        android:background="@drawable/button_round"
                        android:layout_width="160dp"
                        android:layout_height="45dp" />


                </LinearLayout>

            </LinearLayout>
        </ScrollView>

    </RelativeLayout>



</RelativeLayout>